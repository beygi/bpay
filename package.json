    "react-dom": "^16.4.2",
    "react-gravatar": "^2.6.3",
    "react-hot-loader": "^4.3.4",
    "react-json-to-html": "^0.0.3",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-tradingview-widget": "^1.3.0",
    "redux": "^4.0.0",
