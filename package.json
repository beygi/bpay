    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@types/react-share": "^2.1.1",
    "@types/superagent": "^3.8.4",
    "antd-jalali-moment": "^3.1.3",
